<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Evento.
    -->
    <changeSet id="20170213194756-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="reto_id"
                                 baseTableName="evento"
                                 constraintName="fk_evento_reto_id"
                                 referencedColumnNames="id"
                                 referencedTableName="reto"/>

        <addForeignKeyConstraint baseColumnNames="admin_id"
                                 baseTableName="evento"
                                 constraintName="fk_evento_admin_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
